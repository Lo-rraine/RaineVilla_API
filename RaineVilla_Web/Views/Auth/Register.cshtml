@model RaineVilla_Web.Models.Dto.RegistrationRequestDTO

<h1>Login</h1>
<form method="post">
    <div asp-validation-summary="All" class="text-danger"></div>
    <input asp-for="Name" type="text" placeholder="Name" />
    <input asp-for="UserName" type="text" placeholder="Username" />
    <input asp-for="Password" type="text" placeholder="Password" />
    <button type="submit" value="Submit">Register</button>
</form>


@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}